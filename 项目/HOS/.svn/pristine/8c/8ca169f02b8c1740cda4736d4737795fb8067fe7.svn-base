@media only screen and (max-width: 768px) {
    #bg_test{
        background-color: blue;
    }
    /*总容器 隐藏掉为移动端以上设计的信息展示容器*/


    /*************************************/
    /*************手机下的内容展示 begin*****/
    /*****************部分1:搜索栏部分  begin***/
    /*自定义*/
    .doc-info-m{
        width:49%;
        float: left;
    }
    /*自定义*/
    .dep-code-m{
        width:49%;
        float: right;
    }
    /*搜索栏中的按钮容器  begin*****/
    /*搜索栏中的按钮容器  end*****/
    /*****************部分1:搜索栏部分  end***/

    /*****************部分2:列表部分 begin***/
    .item-m{
        width:100%;
        height: 280px;
        /*background-color: #ededed;*/
    }
    .item-m .doc-info-m{
        width:100px;
        height:100%;
        float: left;
        border: 1px solid grey;
    }
    .item-m .doc-info-m img{
        display: block;
        width:90px;
        height: 120px;
        margin: 10px auto;
    }
    .item-m .doc-info-m span{
        display:block;
        text-align: center;
        height: 35px;
        line-height: 35px;
    }
    .item-m .doc-info-m .doc_name_m{
        font-weight: 600;
    }
    .item-m .doc-info-m .dep_name_m{
        color:#3DAFA7;
    }
    .item-m .duty-container-m{
        height:100%;
        padding-left: 100px;
    }
    .item-m .duty-container-m div{
        width:100%;
        height: 35px;
    }
    .item-m .duty-container-m .range{
        border-top: 1px solid grey;
        background-color:#D9EDF7 ;
    }
    .item-m .duty-container-m div span{
        width:33.3%;
        height: 35px;
        line-height: 35px;
        text-align: center;
        float: left;
        border-right: 1px solid grey;
        border-bottom: 1px solid grey;
    }
    /*****************部分2:列表部分 end***/

    /****************部分3：分页部分 begin*/
    /****************部分3：分页部分 end*/
    /*************************************/
    /*************手机下的内容展示 end*****/

}


