@media only screen and (max-width: 768px) {
    #bg_test{
        background-color: blue;
    }
    /*****************轮播组件 begin*******************************/
    /*轮播组件总容器*/
    #mycarousel{
        width:100%;
        height: 180px;
        margin: 0 auto;
        position: relative;
        top:70px;
    }
    /*轮播组件中每一个轮播中的图片*/
    #mycarousel img{
        width:100%;
        height: 180px;
    }
    /*****************轮播组件 end*******************************/

    /******************内容展示  begin*****************/
    .info-content{
        width:100%;
        padding: 0 5px;
    }
    /*****************部分1:搜索栏部分  begin***/
    .ic-search{
        width:100%;
        height: 160px;
    }
    /*自定义*/
    .hos-name{
        width:100%;
    }
    /*自定义*/
    .hos-name span{
        display: none;
    }
    /*自定义*/
    .cant-code{
        width:49%;
        float: left;
    }
    /*自定义*/
    .cant-code span{
        display: none;
    }
    /*自定义*/
    .hoslevel-code{
        width:49%;
        float: right !important;
    }
    /*自定义*/
    .hoslevel-code span{
        display: none;
    }

    /*搜索项中的input*/
    .ic-search input{
        width: 100%;
    }
    /*搜索项中的select*/
    .ic-search select{
        width: 100%;
        margin:0 0px;
    }
    /**********搜索栏中的搜索项 end*/
    /*搜索栏中的按钮容器  begin*****/
    .ic-search .btn-container{
        width:100%;
        height: 50px;
    }
    .btn-search{
        width:100%;
    }
    .btn-search span{
       margin: 0 20px;
        letter-spacing: 10px;
    }
    /*搜索栏中的按钮容器  end*****/
    /*****************部分1:搜索栏部分  end***/

    /*****************部分2:列表部分  begin**/
    .list-container{
        display:none;
    }
    .list-container-m{
        display:block;
        width:100%;
        min-height: 150px;
    }
    /*列表项*/
    .list-item-m{
        width:100%;
        height: auto;
        border: 1px dashed grey;
        min-height: 150px;
    }
    /*列表项中的图片*/
    .list-item-m img{
        width:140px;
        height: 120px;
        float: left;
        position: relative;
        top:15px;
    }
    /*列表项中宽度自适应的混合容器*/
    .list-item-m .mix-container-m{
        height: 100%;
        padding-left: 150px;
        padding-top: 10px;
    }
    .mix-container-m .hos-name-m{
        height: 23px;
        font-size: 16px;
        color:#666;
        font-weight: 600;
    }
    .mix-container-m .hoslevel-code-m{
        height: 28px;
        line-height: 28px;
        text-align: center;
        background-color: #FDE8C9;
        color:#E87404;
    }
    .mix-container-m .cant-code-m{
        height: 28px;
        line-height: 28px;
        text-align: center;
        background-color: #CBF0BA;
        color:#3DAFA7;
        margin-left: 20px;
    }
    .mix-container .msg{
        display: block;
        width:100%;
        float: left;
        margin-top: 20px;
        font-size: 16px;
        line-height: 30px;
    }
    /*****************部分2:列表部分  end**/

    /******************内容展示  end*****************/
}