}
@media only screen and (max-width: 768px) {
    #bg_test{
        background-color: blue;
    }

    /*总容器 隐藏掉为移动端以上设计的信息展示容器*/

    /*************************************/
    /*************手机下的内容展示 begin*****/
    .list-container-m{
        margin-top: 0px;
    }
    /*****************部分1:列表部分 begin***/
    .p_name-m{
        width:49%;
        height:35px;
        line-height:35px;
        float:left;
        text-align: left;
    }
    .p_id-m{
        width:49%;
        height:35px;
        line-height:35px;
        float:right;
        text-align: right;
    }
    .begin_date-m{
        float: left;
        height: 35px;
        line-height: 35px;
    }
    .end_date-m{
        float: left;
        height: 35px;
        line-height: 35px;
    }
    /*删除按钮*/
    .btn-container-m{
        width:49%;
        /*!*height: ;*!*/
        float: right;
    }
    .btn-del-m{
        width:100px;
        height: 35px;
        margin: 0 10px;
        line-height: 35px;
        text-align: center;
        border-radius: 7px;
        float: right;
        position: relative;
        top:0px;
        font-size: 14px;
        font-weight: 600;
    }
    .btn-del-m{
        background-color:#FDE8C9 ;
        color:#E87404;
    }
    .btn-del-m:hover{
        background-color:#fce0b8 ;
        text-decoration: none;
        color:#E87404;
    }
    .btn-del-m img{
        width:28px;
        height: 28px;
        position: relative;
        left:-5px;
        top:-3px;
    }
    .btn-del-m span{
        position: relative;
        top:-2px;
    }
    /*****************部分1:列表部分 end***/

    /*****************部分2:分页部分 begin***/
    /*****************部分2:分页部分 end***/

    /*************************************/
    /*************手机下的内容展示 end*****/

}


