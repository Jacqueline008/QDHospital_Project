@media only screen and (max-width: 768px) {
    #bg_test{
        background-color: blue;
    }

    /*总容器 隐藏掉为移动端以上设计的信息展示容器*/

    /*************************************/
    /*************手机下的内容展示 begin*****/
    /*****************部分1:搜索栏部分  begin***/
    /*自定义*/
    .ic-search-m{
        height:100px !important;
    }
    .ic-search-m .doc-info-m{
        width:49%;
        height:35px;
        line-height: 35px;
        float:left;
        text-align: left;
    }
    /*自定义*/
    .ic-search-m .dep-code-m{
        width:49%;
        height:35px;
        line-height: 35px;
        float:right;
        text-align: left;
    }
    .btn-search-m{
        width: 100%;
    }
    /*搜索栏中的按钮容器  begin*****/
    /*搜索栏中的按钮容器  end*****/
    /*****************部分1:搜索栏部分  end***/

    /*****************部分2:列表部分 begin***/
    .list-container-m{
        height:auto;
    }
    /*自定义*/
    .list-item-m{
        margin: 10px 0;
    }
    .doc_code-m{
        width:33%;
        height: 35px;
        line-height: 35px;
        float: left;
        text-align: left;
    }
    .doc_name-m{
        width:33%;
        height: 35px;
        line-height: 35px;
        float: left;
        text-align: left;
    }
    .dep_name-m{
        width:33%;
        height: 35px;
        line-height: 35px;
        float: right;
        text-align: right;
    }
    /*自定义*/
    .dep_name-m span{
        float: right;
        width:65px;
        height: 28px;
        line-height: 28px;
        text-align: center;
        background-color: #CBF0BA;
        color:#3DAFA7;
        position: relative;
        top:3px;
    }
    .num-m{
        width:49%;
        height: 35px;
        line-height: 35px;
        float: left;
        text-align: left;
    }
    .time_content-m{
        width:49%;
        height: 35px;
        line-height: 35px;
        float: right;
        text-align: right;
    }
    .time_content-m select {
        width: 100%;
        height: 100%;
    }
    /*****************部分2:列表部分 end***/

    /*****************部分3:分页部分 begin***/
    /*****************部分3:分页部分 end***/

    /*************************************/
    /*************手机下的内容展示 end*****/

}


