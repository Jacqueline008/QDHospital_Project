/********************************************************************/
/*******************新增|编辑容器     begin********************************/
#add_container{
    width:970px;
    min-height: 450px;
}
/********部分1:新增|编辑容器中的标题  begin**************/
/********部分1:新增|编辑容器中的标题   end***************/
/********部分2:新增|编辑容器中的字典列表容器  begin*******/
.addlist_container{
    width:900px;
    margin: 0 auto;
}
/*字典列表容器中的行*/
.addlist_row{
    width: 100%;
    height: 70px;
    line-height: 70px;
}
/*行中的列*/
.addlist_col{
    width:25%;
    float: left;
    text-align: center;
}
/*字典列表容器中的标头*/
.addlist_header{
    height: 50px;
    background-color:#F2F2F2 ;
    line-height: 50px;
    font-size: 15px;
    color:#555;
    font-weight: 600;
}
/*行中的select*/
.addlist_row select{
    width:90%;
    height: 35px;
    border-radius: 5px;
    border: 1px solid #555;
}
/*行中的input*/
.addlist_row input{
    width:90%;
    height: 35px;
    border: 1px solid #555;
    border-radius: 5px;
    text-align: center;
}
/*行中的button*/
.addlist_row .btn-del{
    width:180px;
    height: 35px;
    margin: 0 10px;
    line-height: 35px;
    text-align: center;
    border-radius: 7px;
    font-size: 14px;
    font-weight: 600;
    border: none;
}
/*删除按钮*/
.btn-del{
    background-color:#FDE8C9 ;
    color:#E87404;
}
.btn-del:hover{
    background-color:#fce0b8 ;
    text-decoration: none;
    color:#E87404;
}
.btn-del img{
    width:28px;
    height: 28px;
    position: relative;
    left:-10px;
    top:-3px;
}
.btn-del span{
    position: relative;
    top:-2px;
    left: 0px;
    letter-spacing: 5px;
}
/********部分2:新增|编辑容器中的字典列表容器  end*******/
/********部分3:新增|编辑容器中的按钮容器  begin*******/
#add_container .btn-container{
    width:450px;
    height: 135px;
    text-align: center;
    padding-top:50px;
    padding-bottom: 40px;
    margin: 0 auto;
}
.btn-add,.btn-submit{
    width:210px;
    height: 45px;
    line-height: 45px;
    text-align: center;
    border-radius: 7px;
    font-size: 14px;
    font-weight: 600;
    border:none;
}
/*新增一行按钮*/
.btn-add{
    border:none;
    background-color: #ededed;
    color:#666;
    letter-spacing: 5px;
    float: left;
}
.btn-add:hover{
    background-color: #dddddd;
    color:#666;
    text-decoration: none;
}
.btn-add img{
    width:25px;
    height: 25px;
    position: relative;
    left:-15px;
    top:-4px;
}
.btn-add span{
    position: relative;
    top:-2px;
}
/********部分3:新增|编辑容器中的按钮容器  end*******/
/********************************************************************/
/*******************新增|编辑容器     end********************************/


/*媒体查询*/
@media only screen and (max-width: 1200px) {
    #bg_test{
        background-color: gray;
    }
}


@media only screen and (max-width: 992px) {
    #bg_test{
        background-color: pink;
    }
    /********************************************************************/
    /*******************新增|编辑容器     begin********************************/
    #add_container{
        width:750px;
    }
    /********部分1:新增|编辑容器中的标题  begin**************/
    /********部分1:新增|编辑容器中的标题   end***************/

    /********部分2:新增|编辑容器中的字典列表容器  begin*******/
    .addlist_container{
        width:730px;
    }
    /*行中的button*/
    .addlist_row .btn-del{
        width:170px;
    }
    /********部分2:新增|编辑容器中的字典列表容器  end*******/

    /********部分3:新增|编辑容器中的按钮容器  begin*******/
    /********部分3:新增|编辑容器中的按钮容器  end*******/

    /********************************************************************/
    /*******************新增|编辑容器     end********************************/

}
@media only screen and (max-width: 768px) {
    #bg_test{
        background-color: blue;
    }
    /********************************************************************/
    /*******************新增|编辑容器     begin********************************/
    #add_container{
        width:100%;
        min-height: 350px;
    }
    /********部分1:新增|编辑容器中的标题  begin**************/
    /********部分1:新增|编辑容器中的标题   end***************/

    /********部分2:新增|编辑容器中的字典列表容器  begin*******/
    .addlist_container{
        width:99%;
    }
    /*字典列表容器中的行*/
    .addlist_row{
        width: 100%;
        height: 110px;
        line-height: 50px;
        border-bottom: 1px dashed #666;
        padding-top: 5px;
    }
    .dic_item{
        float: left;
        width: 50%;
    }
    .dic_code{
        float: right;
        width: 50%;
    }
    .dic_value{
        float: left;
        width: 50%;
    }
    .option{
        float: right;
        width: 50%;
    }
    /*字典列表容器中的标头*/
    .addlist_header{
        display: none;
    }
    /*行中的select*/
    .addlist_row select{
        width:97%;
        height: 35px;
    }
    /*行中的input*/
    .addlist_row input{
        width:97%;
        height: 35px;
    }
    /*行中的button*/
    .addlist_row .btn-del{
        width:97%;
        height: 35px;
        margin: 0 0px;
        line-height: 35px;
    }
    /********部分2:新增|编辑容器中的字典列表容器  begin*******/

    /********部分3:新增|编辑容器中的按钮容器  begin*******/
    #add_container .btn-container{
        width:95%;
        height: 135px;
        text-align: center;
        padding-top:50px;
        padding-bottom: 40px;
        margin: 0 auto;
    }
    .btn-add,.btn-submit{
        width:48%;
        height: 45px;
        line-height: 45px;
        text-align: center;
        border-radius: 7px;
        font-size: 14px;
        font-weight: 600;
        border:none;
    }
    /*重置按钮*/
    .btn-add{
        border:none;
        background-color: #ededed;
        color:#666;
        letter-spacing: 5px;
        float: left;
    }
    .btn-add:hover{
        background-color: #dddddd;
        color:#666;
        text-decoration: none;
    }
    .btn-add img{
        width:25px;
        height: 25px;
        position: relative;
        left:-15px;
        top:-4px;
    }
    .btn-add span{
        position: relative;
        top:-2px;
    }
    /********部分3:新增|编辑容器中的按钮容器  end*******/

    /********************************************************************/
    /*******************新增|编辑容器     end********************************/
}


