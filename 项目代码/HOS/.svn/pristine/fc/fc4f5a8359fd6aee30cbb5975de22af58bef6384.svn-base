/***********************************************************************/
/*******************信息展示容器     begin********************************/
/*总容器*/
/****************容器左侧部分(列表)******************/
/****************容器右侧部分(内容展示)  begin*******/

/*****************右侧部分1:列表部分  begin**/
/*列表总容器*/
.list-container{
    margin-top: 0;
}
.list-container .list-item{
    height: auto;
    overflow: hidden;
    padding-bottom: 10px;
}
/*自定义*/
.list-container .doc_name,.week_code,.time_range{
    width:80px;
    float: left;
    text-align: center;
}
/*自定义*/
.list-container .begin_date,.end_date,.apply_date{
    width:100px;
    float: left;
    text-align: center;
}
/*自定义*/
.list-container .list-item .apply_date{
    line-height: 30px;
    padding-top: 10px;
}
/*自定义*/
.list-container .refuse_reason{
    width:260px;
    float: left;
    text-align: center;
    line-height: 30px;
    padding: 10px 10px ;
}
/*自定义*/
.list-container .stop_status{
    width:100px;
    float: left;
    text-align: center;
}
/*自定义*/
.list-container .list-item .stop_status{
    padding-top: 20px;
}
.list-container .stop_status .status-wait{
    width:90%;
    margin: 0 auto;
    height: 40px;
    display: block;
    background-color: #FFFF99;
    color:#FF9900;
    line-height: 40px;
}
.list-container .stop_status .status-refuse{
    width:90%;
    margin: 0 auto;
    height: 40px;
    display: block;
    background-color: #FDE8C9;
    color:#E87404;
    line-height: 40px;
}
.list-container .stop_status .status-agree{
    width:90%;
    margin: 0 auto;
    height: 40px;
    display: block;
    background-color: #CBF0BA;
    color:#3DAFA7;
    line-height: 40px;
}
/*列表的head*/
/*列表的item*/
/*****************右侧部分1:列表部分  end**/

/*****************右侧部分2:分页部分  begin**/
/*****************右侧部分2:分页部分  end**/

/****************容器右侧部分(内容展示)  end*******/

/***************************************************/
/*******************信息展示容器     end************/



/*媒体查询*/
@media only screen and (max-width: 1200px) {
    #bg_test{
        background-color: gray;
    }
    /***************************************************/
    /*******************信息展示容器     begin************/
    /*总容器*/
    /****************容器左侧部分(列表)******************/
    /****************容器右侧部分(内容展示)  begin*******/

    /*****************右侧部分1:列表部分  begin**/
    /*自定义*/
    .list-container .doc_name,.week_code,.time_range{
        width:70px;
    }
    /*自定义*/
    .list-container .begin_date,.end_date,.apply_date{
        width:90px;
    }
    /*自定义*/
    .list-container .list-item .apply_date{
        line-height: 30px;
        padding-top: 10px;
    }
    /*自定义*/
    .list-container .refuse_reason{
        width:220px;
        line-height: 40px;
        padding: 0;
    }
    /*自定义*/
    .list-container .list-item .refuse_reason{
        line-height: 30px;
        padding-top: 10px;
    }
    /*自定义*/
    .list-container .stop_status{
        width:100px;
    }
    /*自定义*/
    .list-container .list-item .stop_status{
        padding-top: 20px;
    }
    /*****************右侧部分1:列表部分  end**/

    /****************容器右侧部分(内容展示)  end*******/

    /***************************************************/
    /*******************信息展示容器     end************/
}


