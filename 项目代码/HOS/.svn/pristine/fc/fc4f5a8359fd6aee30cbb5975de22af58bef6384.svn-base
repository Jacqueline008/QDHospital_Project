@media only screen and (max-width: 768px) {
    #bg_test{
        background-color: blue;
    }

    /*总容器 隐藏掉为移动端以上设计的信息展示容器*/

    /*************************************/
    /*************手机下的内容展示 begin*****/

    /*****************部分1:列表部分 begin***/
    .list-container-m{
        margin-top:0;
        height: auto;
    }
    .list-item-m{
        height: auto;
        overflow: hidden;
        padding-top: 20px;
        padding-bottom: 20px;
    }
    /*自定义*/
    .doc_name-m,.week_code-m,.time_range-m,.stop_status-m{
        width:25%;
        float: left;
        height: 35px;
        line-height: 35px;
    }
    .doc_name-m{
        font-weight: 600;
    }
    .stop_status-m .status-wait-m{
        width:80px;
        height: 35px;
        text-align: center;
        float: right;
        background-color: #FFFF99;
        color:#FF9900;
        line-height: 35px;
    }
    .stop_status-m .status-refuse-m{
        width:80px;
        height: 35px;
        text-align: center;
        float: right;
        background-color: #FDE8C9;
        color:#E87404;
        line-height: 40px;
    }
    .stop_status-m .status-agree-m{
        width:80px;
        height: 35px;
        text-align: center;
        float: right;
        background-color: #CBF0BA;
        color:#3DAFA7;
        line-height: 40px;
    }
    /*自定义*/
    .begin_date-m{
        width:49%;
        float: left;
        height: 35px;
        line-height: 35px;
        text-align: left;
    }
    /*自定义*/
    .end_date-m{
        width:49%;
        float: right;
        height: 35px;
        line-height: 35px;
        text-align: right;
    }
    /*自定义*/
    .apply_date-m{
        width:100%;
        float: left;
        height: 35px;
        line-height: 35px;
        text-align: left;
    }
    /*自定义*/
    .refuse_reason-m{
        width:100%;
        float: left;
        text-align: left;
    }
    .begin_date-m span,.end_date-m span,.apply_date-m span,.refuse_reason-m span{
        font-weight: 600;
        color:#888;
    }
    /*****************部分1:列表部分 end***/

    /*****************部分2:分页部分 begin***/
    /*****************部分2:分页部分 end***/

    /*************************************/
    /*************手机下的内容展示 end*****/

}


