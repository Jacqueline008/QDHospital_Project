/*****************右侧部分2:列表部分  begin**/
/*自定义*/
.head{
    height: 50px;
    background-color:#D9EDF7 ;
    line-height: 50px;
    font-size: 15px;
    color:#555;
    font-weight: 600;
}
.head .doc-info{
    float: left;
    width:200px;
    text-align: center;
}
.head .duty-container{
    padding-left: 200px;
    height: 50px;
}
.head .duty-container span{
    float: left;
    width:12.5%;
    height: 100%;
    text-align: center;
}

/*自定义*/
.item{
    height: 150px;
    line-height: 50px;
    font-size: 15px;
    color:#555;
    font-weight: 600;
}
.item .doc-info{
    float: left;
    width:200px;
    height: 100%;
    text-align: center;
    border-left: 1px solid grey;
    border-right: 1px solid grey;
    border-bottom: 1px solid grey;
}
.item .doc-info img{
    width:90px;
    height: 120px;
    position: relative;
    top:20px;
    left:-50px;
}
.item .doc-info .doc_name,.doc_code,.dep_name{
    width:100%;
    height: 30px;
    position: relative;
    top:-115px;
    left:5px;
    float:left;
    padding-left: 80px;
    font-weight: 500;
    font-size: 14px;
    margin-top: 10px;
}
.item .doc-info .doc_name{
    font-weight: 600;
}
.item .doc-info .dep_name{
    color:#3DAFA7;
}
.item .duty-container{
    padding-left: 200px;
    height: 150px;
}
.item .duty-container span{
    float: left;
    width:12.5%;
    height: 100%;
    text-align: center;
    border-right: 1px solid grey;
    border-bottom: 1px solid grey;
    font-weight: 500;
}
.item .duty-container .up:hover{
    background-color: #f0f0f0;
}
.item .duty-container .down:hover{
    background-color: #f0f0f0;
}
.item .duty-container .range .up:hover{
    background-color: #fff !important;
}
.item .duty-container .range .down:hover{
    background-color: #fff !important;
}
.item .duty-container span .up{
    display: block;
    width:100%;
    height: 50%;
    border-bottom: 1px solid grey;
    line-height: 75px;
}
.item .duty-container span .down{
    display: block;
    width:100%;
    height: 50%;
    line-height: 75px;
}
/*****************右侧部分2:列表部分  end**/

/*****************右侧部分3:分页部分  begin**/
/*****************右侧部分3:分页部分  end**/

