#bg_test{
    position: relative;
    top:180px;
    background-color: purple;
}
body{
    background-color: #f4f6f8;
}

/***********************************************************************/
/*******************模块展示容器     begin********************************/
#module_container{
    width:970px;
    height: 450px;
    margin: 0 auto;
    position:relative;
    top:120px;
}
/*********模块项 begin***********/
.item-contaner{
    width:300px;
    height:170px;
    background-color: #fff;
    text-align: center;
    float: left;
    margin-top: 30px;
    display: block;
    color:#333;
}
.item-contaner:hover{
    color:#333;
    text-decoration: none;
}
.middle{
    margin-left:35px;
    margin-right: 35px;
}
.item-content{
    width:210px;
    height: 45px;
    margin: 0 auto;
    position: relative;
    top:60px;
}
/*模块item-content中的img*/
.item-content img{
    width:45px;
    height: 45px;
    float: left;
}
/*模块item-content中的item-title*/
.item-content .item-title{
    font-weight: 600;
    font-size: 16px;
    display: block;
    text-align: left;
    padding-left: 70px;
}
/*模块item-content中的item-msg*/
.item-content .item-msg{
    font-size: 14px;
    display: block;
    text-align: left;
    padding-left: 70px;
}
/*********模块项 end***********/
/***********************************************************************/
/*******************模块展示容器     end********************************/


/*媒体查询*/
@media only screen and (max-width: 1200px) {
    #bg_test{
        background-color: gray;
    }
}


@media only screen and (max-width: 992px) {
    #bg_test{
        background-color: pink;
    }
    /***********************************************************************/
    /*******************模块展示容器     begin********************************/
    #module_container{
        width:750px;
        height: 630px;
        top:80px;
    }
    /*********模块项 begin***********/
    .item-contaner{
        width:330px;
        height:170px;
        margin-top: 30px;
        margin-left: 20px;
        margin-right: 20px;
    }
    .middle{
        margin-left:20px;
        margin-right: 0px;
    }
    /*********模块项 end***********/
    /***********************************************************************/
    /*******************模块展示容器     end********************************/

}
@media only screen and (max-width: 768px) {
    #bg_test{
        background-color: blue;
    }
    /***********************************************************************/
    /*******************模块展示容器     begin********************************/
    #module_container{
        width:100%;
        height: 630px;
        top:100px;
    }
    /*********模块项 begin***********/
    .item-contaner{
        width:95%;
        height:80px;
        float: none;
        margin:20px auto;
    }
    .first{
        margin-top:0px;
    }
    .middle{
        margin:0px auto;
    }
    .item-content{
        width:250px;
        height: 45px;
        margin: 0 auto;
        position: relative;
        top:20px;
    }
    /*模块item-content中的img*/
    .item-content img{
        width:45px;
        height: 45px;
        float: left;
    }
    /*模块item-content中的item-title*/
    .item-content .item-title{
        font-weight: 600;
        font-size: 16px;
        display: block;
        text-align: left;
        padding-left: 70px;
    }
    /*模块item-content中的item-msg*/
    .item-content .item-msg{
        font-size: 14px;
        display: block;
        text-align: left;
        padding-left: 70px;
    }
    /*********模块项 end***********/
    /***********************************************************************/
    /*******************模块展示容器     end********************************/

}


