/*****************右侧部分1:列表部分  begin**/
/*列表总容器*/
/*自定义*/
.list-container .hos_code,.cant_name,.hoslevel_name{
    width:150px;
    float: left;
    text-align: center;
}
/*自定义*/
.list-container .p_name,.hos_name,.dep_name,.doc_name{
    width:140px;
    float: left;
    text-align: center;
}
.t_date{
    width:190px;
    float: left;
    text-align: center;
}
/*列表的head*/
/*列表的item*/
/**********列表中的按钮容器  begin*****/
.btn-container{
    width:150px !important;
}
.btn-del{
    width:130px;
}
/**********列表中的按钮容器  end*****/
/*****************右侧部分1:列表部分  end**/

/*****************右侧部分2:分页部分  begin**/
/*****************右侧部分2:分页部分  end**/

