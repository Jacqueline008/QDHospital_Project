    /*****************部分2:列表部分 begin***/
    .info-content-mobile .ic-search-m {
        width: 100%;
        height: 100px;
    }
    /*自定义*/
    .dic_item-m{
        width: 50%;
        height: 35px;
        line-height: 35px;
        float: left;
        color:#3DAFA7;
    }
    /*自定义*/
