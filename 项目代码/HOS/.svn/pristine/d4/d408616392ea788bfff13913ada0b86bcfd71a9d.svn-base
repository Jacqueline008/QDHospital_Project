/*****************右侧部分1:搜索栏部分  begin***/

/********搜索栏中的搜索项 begin*/
#info-container{
    height: 700px;
}
/*自定义*/
.dep-info{
    width:320px;
    text-align: left;
}
/*自定义*/
.dep-info input{
    width:200px;
}
/*自定义*/
.dic-value{
    width:320px;
    text-align: left;
}
/*自定义*/
.dic-value select{
    width:230px;
}
/**********搜索栏中的搜索项 end*/

/*搜索栏中的按钮容器  begin*****/
/*搜索栏中的按钮容器  end*****/

/*****************右侧部分1:搜索栏部分  end***/

/*****************右侧部分2:列表部分  begin**/
/*自定义*/
.list-item{
    height: 100px;
    line-height: 100px;
}
/*自定义*/
.list-container .dep_code,.dep_name ,.dic_value{
    width:100px;
    float: left;
    text-align: center;
}
/*自定义*/
.list-container .dep_description{
    width:350px;
    float: left;
    text-align: center;
}
/*自定义*/
.list-item .dep_description{
    font-size: 12px;
    line-height: 20px;
    height: 100%;
    padding-top: 10px;
    word-wrap: break-word;
}
/**********列表中的按钮容器  begin*****/
/*自定义*/
.btn-edit,.btn-del{
    top:37px;
}
/**********列表中的按钮容器  end*****/
